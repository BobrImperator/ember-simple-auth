{
  "name": "simple-auth-server",
  "now": {
    "version": 2,
    "routes": [
      {
        "src": "/.*",
        "dest": "/app.js",
        "methods": [
          "GET",
          "POST"
        ]
      }
    ],
    "builds": [
      {
        "src": "app.js",
        "use": "@now/node"
      }
    ],
    "build": {
      "env": {
        "NODEJS_HELPERS": "0"
      }
    }
  },
  "dependencies": {
    "body-parser": "^1.17.2",
    "cors": "^2.8.1",
    "express": "^4.17.1",
    "glob": "^7.1.1",
    "morgan": "^1.8.1"
  }
}
